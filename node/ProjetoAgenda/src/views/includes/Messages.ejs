<%  if(errors.length > 0){       %>

<div class="row">
    <div class="col my-3">
      <div class="alert alert-danger">
       
        <%  errors.forEach(err => {  %> 
        <%= err %> <br>
        <% }) %>
      </div>
    </div>
</div>
<% }  %>
<%  if(succes.length > 0){       %>

  <div class="row">
      <div class="col my-3">
        <div class="alert alert-success">
         
          <%  succes.forEach(succ => {  %> 
          <%= succ %> <br>
          <% }) %>
        </div>
      </div>
  </div>
  <% }  %>
  
